{
  "extends": ["react-app"],
  "rules": {
    "prefer-template": "off",
    "no-useless-concat": "off",
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "interface",
        "format": ["PascalCase"]
      }
    ]
  }
}
